<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.TilePane?>

<fx:root type="BorderPane" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <TilePane fx:id="tilePanel" hgap="6.0" tileAlignment="TOP_LEFT" vgap="6.0" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="6.0" left="6.0" right="6.0"/>
            </BorderPane.margin>
        </TilePane>
    </center>
</fx:root>
