<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<fx:root type="BorderPane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <HBox fx:id="menu" alignment="CENTER_LEFT" prefHeight="40.0" spacing="6.0" styleClass="menu-bar" BorderPane.alignment="CENTER">
         <children>
         </children>
         <padding>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </padding>
      </HBox>
   </top>
   <bottom>
      <HBox alignment="CENTER_RIGHT" spacing="12.0" styleClass="status-bar" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </padding>
         <children>
            <Label fx:id="lblVersion" styleClass="status-bar-label" />
         </children>
      </HBox>
   </bottom>
</fx:root>
