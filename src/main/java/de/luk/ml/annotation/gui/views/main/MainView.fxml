<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<fx:root type="BorderPane" xmlns="http://javafx.com/javafx/8.0.101" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <HBox alignment="CENTER" spacing="10.0">
            <children>
                <Button styleClass="main-button" mnemonicParsing="false" onAction="#createAnnotation" text="Create annotation">
                </Button>
                <Button styleClass="main-button" mnemonicParsing="false" onAction="#reviewAnnotations" text="Review annotation">
                </Button>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
    </center>
</fx:root>
